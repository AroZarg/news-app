      },
      "package": "com.arazarg.newsapp",
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5a51ac6f-408a-4e45-a4c2-913d608c8971"
      }
    }
  }
